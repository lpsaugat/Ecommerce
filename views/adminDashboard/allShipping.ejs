<div class="bg-[#f8f8f8] pr-5">
  <div class="hidden"><%- include ("../partials/header") %></div>
  
  <div class="flex max-w-1200 mx-auto">
    <div class="grid gap-x-3 grid-cols-4 w-full rounded-sm">
      <%- include ("./sidebar") %>
  
        <div class="grid col-span-3 grid-cols-3 gap-x-1 py-3">
          
          <div class="grid col-span-3 grid-cols-3 gap-x-1">
            <div
        class=" bg-white rounded-md p-3 flex-row justify-between col-span-3 gap-x-[9px] md:gap-x-[12px] lg:gap-x-[9px] font-normal"
      >
    <div class="flex justify-end gap-x-2">
    
      <div class="h-[42px] text-lg items-center text-center hidden md:flex">Go To</div>
       <input
          type="number"
          class=" h-[36px] w-[60px] border-[1px] text-lg font-medium rounded-[3px] items-center text-center justify-center md:flex hidden"
       min="1" 
       max="<%=dataPagination.totalPages%>"
       value="<%=dataPagination.page%>"
       id="pageNumber"/>
       
      </div>
      <div class="flex w-full justify-end mt-3"> <button
        onclick="adminPages('allShipping')"
        class="p-3 h-[36px] text-lg items-center flex justify-center border-[1px] rounded-[3px]"
      >
        Submit
      </button></div>
      
      
    </div>
              <div class="mt-2 col-span-3">
                
                <div
                class="rounded-t-md w-full col-span-2 text-[17px] text-[#000123] bg-white border-b-[1px]"
              >
                <div
                  class="justify-between items-center flex  px-[10px] md:px-[11px] lg:px-[17px] rounded-[3px] lg:shadow-none shadow-[0px_0px_2px_rgba(0,0,0,0.25)]"
                >
                  <div class="flex w-full md:h-[86px]">
                    <div
                      class="md:w-[86px] w-[69px] justify-center flex items-center ml-[10px]"
                    >
                      Name
                    </div>
                    <div
                      class="pl-[10px] md:pl-[10px] flex flex-col lg:font-medium text-ellipsis truncate lg:overflow-visible lg:flex-row w-full lg:grid grid-cols-12 lg:items-center"
                    >
                      <div
                        class="text-[12px] cursor-pointer md:text-sm lg:text-[16px] col-span-4 lg:pl-[40px] items-center flex"
                        onclick="toggleSort('user')" >
                        UserID
                      </div>
              
                      <div
                        class="text-[12px] cursor-pointer col-span-2 w-auto md:text-[16px] lg:pt-0 font-medium pt-[14px] md:pt-[17px] justify-between items-center grid-cols-1 flex"
                        onclick="toggleSort('total')" >
                        Total
                      </div>
                      <form
                        class="col-span-2 cursor-pointer justify-between items-center flex text-[16px] pt-[17px] deliveryStatus"
                        onclick="toggleSort('shippingStatus')">
                        
                        Status
                      </form>
                      <div class="lg:flex font-roboto flex items-center left-0 w-auto">
                        <div class="flex items-center">
                          <div class="flex justify-center">
                            
                          </div>
                        </div>
                      </div>
                      <div
                        class="text-[12px] justify-between col-span-3 w-auto md:text-[16px] lg:pt-0 font-medium pt-[14px] md:pt-[17px] items-center grid-cols-2 flex"
                      >
                        <div class="pl-1 flex flex-col items-center">
                          <div class="text-[14px] cursor-pointer" onclick="toggleSort('createdAt')">
                            Created
                          </div>
                        </div>
                        <div class="pl-1 flex flex-col items-center">
                          <div class="text-[14px] cursor-pointer" onclick="toggleSort('updatedAt')">
                            Updated
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
                  <% for(i=0;i<shipping.length;i++) {%>
                    <%- include("./ShippingItems")%> <%}%>
                </div>
          </div>
        </div>
  
        <div class="hidden"><%- include ("../partials/footer") %></div>
      </div>
    </div>
  </div>
  